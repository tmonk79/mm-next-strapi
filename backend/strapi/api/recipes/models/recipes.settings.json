{
  "kind": "collectionType",
  "collectionName": "recipes",
  "info": {
    "name": "Recipes"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Recipe_Video_Url": {
      "type": "text"
    },
    "Image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "Difficulty": {
      "type": "enumeration",
      "enum": [
        "Easy",
        "Medium",
        "Hard",
        "Pro"
      ]
    },
    "Time": {
      "type": "string"
    },
    "Serves": {
      "type": "integer"
    },
    "Recipe_Servers_List": {
      "type": "text"
    },
    "Ingredient_Tags": {
      "type": "text"
    },
    "Relation_Products": {
      "collection": "recipes"
    },
    "Is_Veg": {
      "type": "boolean"
    },
    "recipeCategories": {
      "collection": "category",
      "via": "recipes",
      "dominant": true
    },
    "recipe_iingridents": {
      "collection": "recipe-ingredient",
      "via": "recipes",
      "dominant": true
    },
    "recipe_steps": {
      "collection": "recipe-steps",
      "via": "recipes",
      "dominant": true
    }
  }
}
